{
    "manifest_version": 3,
    "version": "1.0",
    "description": "just for testing and learning",
     "icons": {
        "16": "/images/icon-16x16.png",
        "19": "/images/icon-16x16.png",
        "38": "/images/icon-16x16.png",
        "48": "/images/icon-16x16.png",
        "128": "/images/icon-16x16.png"
    },
    "name": "Test is the Name",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "cookies",
        "tabs",
        "scripting",
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "*://*/",
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "My Test default"
    }


}
